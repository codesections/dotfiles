config_version: 2
settings:
  bindings.commands:
    global:
      command:
        <Alt+n>: command-history-next
        <Alt+p>: command-history-prev
        <Ctrl+g>: leave-mode
        <Ctrl+n>: completion-item-focus next
        <Ctrl+p>: completion-item-focus prev
        <Ctrl+r>: search-prev
        <Ctrl+s>: search-next
      insert:
        <Ctrl+6>: tab-focus last
        <Ctrl+b>: scroll left
        <Ctrl+e>: open-editor
        <Ctrl+f>: scroll right
        <Ctrl+n>: scroll down
        <Ctrl+p>: scroll up
        <Ctrl+u><Ctrl+x><Ctrl+f>: set-cmd-text -s :open
        <Ctrl+x><Ctrl+c>: quit --save
        <Ctrl+x><Ctrl+f>: set-cmd-text -s :open -t
        <Ctrl+x><Ctrl+m>: 'set-cmd-text :'
        <Ctrl+x><Ctrl+s>: session-save
        <Ctrl+x>[: tab-prev
        <Ctrl+x>]: tab-next
        <Ctrl+x>b: set-cmd-text -s :buffer
        <Ctrl+x>k: tab-close
      normal:
        '''': null
        '''Z': :buffer https://qutebrowser.org/doc/help/configuring.html
        <Ctrl+6>: tab-focus last
        <Ctrl+Shift+o>: set-cmd-text -s :open
        <Ctrl+b>: scroll left
        <Ctrl+e>: open-editor
        <Ctrl+f>: scroll right
        <Ctrl+h>: null
        <Ctrl+j>: forward
        <Ctrl+k>: back
        <Ctrl+n>: scroll down
        <Ctrl+o>: set-cmd-text -s :open -t
        <Ctrl+p>: scroll up
        <Ctrl+s>: set-cmd-text /
        <Ctrl+u>: null
        <Ctrl+u><Ctrl+x><Ctrl+f>: set-cmd-text -s :open
        <Ctrl+w>: null
        <Ctrl+x>: null
        <Ctrl+x><Ctrl+c>: quit --save
        <Ctrl+x><Ctrl+f>: set-cmd-text -s :open -t
        <Ctrl+x><Ctrl+m>: 'set-cmd-text :'
        <Ctrl+x><Ctrl+s>: session-save
        <Ctrl+x>[: tab-prev
        <Ctrl+x>]: tab-next
        <Ctrl+x>b: set-cmd-text -s :buffer
        <Ctrl+x>k: tab-close
        <Tab>: nop
        J: null
        K: null
        '[b': tab-prev
        ']b': tab-next
        co: null
        j: scroll down
        k: scroll up
        m: null
        mZ: set-cmd-text :bind 'Z :buffer {url}
        tb: set-cmd-text -s :tab-take
      passthrough:
        <Ctrl+6>: tab-focus last
        <Ctrl+b>: scroll left
        <Ctrl+e>: open-editor
        <Ctrl+f>: scroll right
        <Ctrl+n>: scroll down
        <Ctrl+p>: scroll up
